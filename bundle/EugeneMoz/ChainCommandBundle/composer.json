{
  "name": "eugenemoz/chain-command-bundle",
  "version": "0.0.1",
  "type": "symfony-bundle",
  "description": "Test command chaining bundle",
  "license": "MIT",
  "require": {
    "php": ">=8.2",
    "symfony/monolog-bundle": "v3.10.0",
    "symfony/console":  "7.2.*",
    "symfony/yaml": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "7.2.*",
    "symfony/css-selector": "7.2.*",
    "symfony/phpunit-bridge": "^7.2",
    "symfony/framework-bundle": "^7.2",

    "symfony/filesystem": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "EugeneMoz\\ChainCommandBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EugeneMoz\\ChainCommandBundle\\Tests\\": "tests/"
    }
  }
}
